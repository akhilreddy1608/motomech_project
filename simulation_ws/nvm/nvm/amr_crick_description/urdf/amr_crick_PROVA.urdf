<?xml version="1.0"?>
<robot name="amr_crick">

    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/amr_crick</robotNamespace>
        </plugin>
    </gazebo>

    <material name="green">
        <color rgba="0 0.8 0 1"/>
    </material>

    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <!-- base_link START -->
    <link name="base_link">
    </link>

     <link name="body_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.033" />
            <inertia ixx="67e-6" ixy="0.0" ixz="0.0" iyy="84e-6" iyz="0.0" izz="131e-6"/>
        </inertial>
       
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.040 0.040 0.020"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                 <box size="0.040 0.040 0.020"/>
            </geometry>
            <material name="green"/>
        </visual>

    </link>

    <joint name="base_joint" type="fixed">
        <parent link="base_link"/>
        <child link="body_link"/>
        <origin xyz="0.0 0 0.0" rpy="0 0 0"/>
    </joint>

    <gazebo reference="body_link">
        <kp>10000.0</kp>
        <kd>10000.0</kd>
        <mu1>10.0</mu1>
        <mu2>10.0</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>

    <!-- base_link END -->


    <!-- rods_sx_down_link START -->

    <link name="rods_sx_down_link">

        <inertial>
            <origin xyz="0 -0.039 0" rpy="0 0 0"/>
            <mass value="0.089" />
            <inertia ixx="75e-6" ixy="0.0" ixz="0.0" iyy="142e-6" iyz="0.0" izz="216e-6"/>
        </inertial>
       
        <collision>
            <origin xyz="0 -0.038 0" rpy="0 0 0"/>
            <geometry>
                <box size = "0.010 0.078 0.010"/>
             <!--    <mesh filename="package://amr_crick_description/models/meshes/amr_crick_rods_sx_down.dae"/> -->
            </geometry>
        </collision> 

        <visual>
            <origin  xyz="0 -0.038 0" rpy="0 0 0"/>
            <geometry>
            <box size = "0.010 0.078 0.010"/>
             <!--    <mesh filename="package://amr_crick_description/models/meshes/amr_crick_rods_sx_down.dae"/> -->
            </geometry>
             <material name="red"/>
        </visual>
    </link>

    <gazebo reference="rods_sx_down_link">
        <kp>10000.0</kp>
        <kd>10000.0</kd>
        <mu1>10.0</mu1>
        <mu2>10.0</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>

    <!-- rods_sx_down_link END -->

    <!-- 1/ roll_1 START -->        
    <joint name="roll_1_joint" type="revolute">
        <parent link="body_link"/>
        <child link="rods_sx_down_link"/>
        <origin xyz="0.026 -0.017 -0.004" rpy="-0.24 0 0"/>
        <limit lower="-3.14159" upper="3.14159" effort="1000" velocity="0.005"/>
        <axis xyz="1 0 0"/>
    </joint>

    <transmission name="tran1">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="roll_1_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor1">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <!-- roll_1 END -->   

    <!-- amr_crick_mozzo_motore_link START --> 
    <link name="amr_crick_mozzo_motore_link">

        <inertial>
            <origin xyz="-0.026 0 0" rpy="0 0 0"/>
            <mass value="0.1" />
            <inertia ixx="400e-6" ixy="0.0" ixz="0.0" iyy="200e-6" iyz="0.0" izz="400e-6"/>
        </inertial>
       
     
        <collision>
            <origin xyz="-0.026 0 0" rpy="-0.026 0 0"/>
            <geometry>
                <box size="0.045 0.035 0.042"/>
                <!--  <mesh filename="package://amr_crick_description/models/meshes/amr_crick_mozzo_motore.dae"/>-->
            </geometry> 
        </collision>   

        <visual>
           <origin xyz="-0.026 0 0" rpy="-0.026 0 0"/>
            <geometry>
               <!--  <mesh filename="package://amr_crick_description/models/meshes/amr_crick_mozzo_motore.dae"/>--> 
                <box size="0.045 0.035 0.042"/>
            </geometry>
             <material name="red"/>
        </visual>
    </link>

    <gazebo reference="amr_crick_mozzo_motore_link">
        <kp>10000.0</kp>
        <kd>10000.0</kd>
        <mu1>10.0</mu1>
        <mu2>10.0</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>

    <!-- amr_crick_mozzo_motore_link END -->

    <!-- 2/ roll_2 START -->        
    <joint name="roll_2_joint" type="revolute">
        <parent link="rods_sx_down_link"/>
        <child link="amr_crick_mozzo_motore_link"/>
        <origin xyz="0 -0.078 0" rpy="0.24 0 0"/>
        <limit lower="-3.14159" upper="3.14159" effort="1000" velocity="0.005"/>
        <axis xyz="1 0 0"/>
    </joint>

    <transmission name="tran2">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="roll_2_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor2">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
    <!-- roll_2 END --> 
    <!-- rods_sx_up_link START -->
    <link name="rods_sx_up_link">

        <inertial>
             <origin  xyz="0 0.038 0" rpy="0 0 0"/>
            <mass value="0.089" />
            <inertia ixx="75e-6" ixy="0.0" ixz="0.0" iyy="142e-6" iyz="0.0" izz="216e-6"/>
        </inertial>
       
        <collision>
             <origin  xyz="0 0.038 0" rpy="0 0 0"/>
            <geometry>
                <box size = "0.010 0.078 0.010"/>
             <!--    <mesh filename="package://amr_crick_description/models/meshes/amr_crick_rods_sx_down.dae"/> -->
            </geometry>
        </collision> 

        <visual>
            <origin  xyz="0 0.038 0" rpy="0 0 0"/>
            <geometry>
            <box size = "0.010 0.078 0.010"/>
             <!--    <mesh filename="package://amr_crick_description/models/meshes/amr_crick_rods_sx_down.dae"/> -->
            </geometry>
             <material name="red"/>
        </visual>
    </link>

    <gazebo reference="rods_sx_up_link">
        <kp>1000.0</kp>
        <kd>1000.0</kd>
        <mu1>10.0</mu1>
        <mu2>10.0</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>

    <!-- rods_sx_up_link END -->

    <!-- 3/ roll_3 START -->        
    <joint name="roll_3_joint" type="revolute">
        <parent link="amr_crick_mozzo_motore_link"/>
        <child link="rods_sx_up_link"/>
        <origin xyz="0.010 0 0" rpy="0.24 0 0"/>
        <limit lower="-3.14159" upper="3.14159" effort="1000" velocity="0.005"/>
        <axis xyz="1 0 0"/>
    </joint>

        <transmission name="tran3">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="roll_3_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor3">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
   
    <!-- roll_3 END --> 
    <!--body up start --> 

         <link name="body_up_link">
        <inertial>
           <origin rpy="0 0 0" xyz="-0.025 0 0"/>
            <mass value="0.033" />
            <inertia ixx="67e-6" ixy="0.0" ixz="0.0" iyy="84e-6" iyz="0.0" izz="131e-6"/>
        </inertial>
       
        <collision>
           <origin rpy="0 0 0" xyz="-0.025 0 0"/>
            <geometry>
                <box size="0.040 0.040 0.020"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="-0.025 0 0"/>
            <geometry>
                 <box size="0.040 0.040 0.020"/>
            </geometry>
            <material name="green"/>
        </visual>

    </link>

    <gazebo reference="body_up_link">
        <kp>10000.0</kp>
        <kd>10000.0</kd>
        <mu1>10.0</mu1>
        <mu2>10.0</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>


    <!-- 4/ roll_4 START -->        
    <joint name="roll_4_joint" type="revolute">
        <parent link="rods_sx_up_link"/>
        <child link="body_up_link"/>
        <origin xyz="0 0.078 0" rpy="-0.24 0 0"/>
        <limit lower="-3.14159" upper="3.14159" effort="1000" velocity="0.005"/>
        <axis xyz="1 0 0"/>
    </joint>

        <transmission name="tran4">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="roll_4_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor4">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    </robot> 